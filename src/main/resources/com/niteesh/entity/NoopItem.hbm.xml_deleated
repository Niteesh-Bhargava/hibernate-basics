<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.niteesh.entity.NoopItem" table="noop_item">
        <id name="id" type="long" column="item_id">
            <generator class="native" />
        </id>
        <property name="name" type="string" column="item_name" />
        <property name="validated" type="java.lang.Boolean"
                  column="validated" access="noop" />
    </class>
</hibernate-mapping>